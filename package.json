{
  "name": "docwhat.org",
  "version": "1.0.0",
  "description": "Personal blog of Christian Höltje",
  "keywords": [
    "blog",
    "gatsby"
  ],
  "homepage": "https://github.com/docwhat/website#readme",
  "bugs": {
    "url": "https://github.com/docwhat/website/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docwhat/website.git"
  },
  "license": "MIT",
  "author": "Christian Höltje (https://docwhat.org/)",
  "main": "n/a",
  "scripts": {
    "build": "NODE_ENV=production script/env gatsby build",
    "clean": "gatsby clean",
    "compress": "script/compress",
    "cypress:open": "cypress open",
    "cypress:run": "wait-on http://localhost:8000 && cypress run",
    "cypress:run:chrome": "cypress:run --browser=chrome",
    "cypress:run:firefox": "cypress:run --browser=firefox",
    "cypress:run:headed": "cypress:run --headed",
    "cypress:run:record": "cypress:run --record",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "develop": "NODE_ENV=development script/env gatsby develop",
    "docker:build": "npm run build && npm run compress && script/build",
    "docker:run": "docker container run --rm --detach --publish=8000:80 --name=docwhat.org docwhat:local",
    "docker:stop": "docker container rm -f docwhat.org",
    "format": "npm run format:prettier && npm run format:packagejson && npm run format:npmlock",
    "format:fix": "npm run format:prettier:fix && npm run format:packagejson:fix && npm run format:npmlock:fix",
    "format:npmlock": "npm dedupe --legacy-peer-deps --dry-run",
    "format:npmlock:fix": "npm dedupe --legacy-peer-deps",
    "format:packagejson": "sort-package-json --check",
    "format:packagejson:fix": "sort-package-json",
    "format:prettier": "prettier --check '**/*.{js,jsx,md,yml,yaml,css,graphql,json,json5,html}'",
    "format:prettier:fix": "prettier --write '**/*.{js,jsx,md,yml,yaml,css,graphql,json,json5,html}'",
    "postinstall": "husky install",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:textlint",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:stylelint:fix && npm run lint:textlint:fix",
    "lint:stylelint": "stylelint --aei './**/*.{html,css}'",
    "lint:stylelint:fix": "stylelint --aei --fix './**/*.{html,css}'",
    "lint:textlint": "[ -n \"${CI-}\" ] || npx textlint .",
    "lint:textlint:fix": "[ -n \"${CI-}\" ] || npx textlint --fix .",
    "new": "node src/utils/newpost.js",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@svgr/webpack": "^6.3.1",
    "bluebird": "~3.7.2",
    "caniuse-db": "~1.0.30001376",
    "chalk": "^5.0.1",
    "color": "^4.2.3",
    "fbjs": "~3.0.4",
    "feed": "~4.2.2",
    "gatsby": "^4.21.0",
    "gatsby-cli": "^4.21.0",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^4.21.0",
    "gatsby-plugin-emotion": "^7.20.0",
    "gatsby-plugin-google-analytics": "^4.21.0",
    "gatsby-plugin-image": "^2.21.0",
    "gatsby-plugin-react-helmet": "^5.20.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^4.20.0",
    "gatsby-plugin-sitemap": "^5.21.0",
    "gatsby-plugin-svgr": "~3.0.0-beta.0",
    "gatsby-remark-autolink-headers": "^5.20.0",
    "gatsby-remark-copy-linked-files": "^5.21.0",
    "gatsby-remark-embed-snippet": "^7.20.0",
    "gatsby-remark-embed-youtube": "0.0.7",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "^6.21.0",
    "gatsby-remark-prismjs": "^6.21.0",
    "gatsby-remark-responsive-iframe": "^5.20.0",
    "gatsby-remark-smartypants": "^5.20.0",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^4.20.0",
    "gatsby-transformer-remark": "^5.20.0",
    "gatsby-transformer-sharp": "^4.21.0",
    "gatsby-transformer-yaml": "^4.20.0",
    "js-yaml": "^4.1.0",
    "lodash": "~4.17.21",
    "moment": "~2.29.4",
    "prismjs": "~1.28.0",
    "prop-types": "~15.8.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-fontawesome": "~1.7.1",
    "react-gravatar": "~2.6.3",
    "react-helmet": "~6.1.0",
    "react-hot-loader": "~4.13.0",
    "react-refresh": "0.14.0",
    "remark": "^14.0.2",
    "replace-ext": "~2.0.0",
    "shevyjs": "^2.1.0",
    "strip-markdown": "~5.0.0",
    "uuid": "~8.3.2",
    "vfile": "~5.3.4",
    "webpack": "~5.73.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@emotion/eslint-plugin": "~11.10.0",
    "@testing-library/cypress": "~8.0.3",
    "cypress": "~10.4.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gatsby-plugin-react-helmet-canonical-urls": "~1.4.0",
    "gatsby-remark-external-links": "0.0.4",
    "husky": "^8.0.1",
    "inquirer": "~9.1.0",
    "inquirer-datepicker-prompt": "0.4.3",
    "lint-staged": "~13.0.3",
    "markdown-link-check": "^3.10.2",
    "object-assign": "~4.1.1",
    "prettier": "~2.7.1",
    "remark-cli": "~11.0.0",
    "remark-frontmatter": "~4.0.1",
    "remark-github": "~11.2.4",
    "remark-preset-github": "~4.0.4",
    "sort-package-json": "~1.57.0",
    "stylelint": "~14.10.0",
    "stylelint-config-prettier": "~9.0.3",
    "stylelint-config-recommended": "~8.0.0",
    "stylelint-prettier": "~2.0.0",
    "textlint": "~12.2.1",
    "textlint-filter-rule-comments": "~1.2.2",
    "textlint-rule-date-weekday-mismatch": "~1.0.6",
    "textlint-rule-diacritics": "~1.0.0",
    "textlint-rule-editorconfig": "~1.0.3",
    "textlint-rule-no-start-duplicated-conjunction": "~2.0.2",
    "textlint-rule-no-todo": "~2.0.1",
    "typescript": "~4.7.4",
    "wait-on": "~6.0.1"
  },
  "packageManager": "npm@7.22.0",
  "engines": {
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
