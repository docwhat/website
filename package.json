{
  "name": "docwhat.org",
  "version": "1.0.0",
  "description": "Personal blog of Christian Höltje",
  "keywords": [
    "blog",
    "gatsby"
  ],
  "homepage": "https://github.com/docwhat/docwhat#readme",
  "bugs": {
    "url": "https://github.com/docwhat/docwhat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docwhat/docwhat.git"
  },
  "license": "MIT",
  "author": "Christian Höltje (https://docwhat.org/)",
  "main": "n/a",
  "scripts": {
    "build": "SITE_COMMIT=\"${SITE_COMMIT:-$(git log -1 --oneline --decorate)}\" SITE_VERSION=\"${SITE_VERSION:-$(git rev-parse HEAD)}\" gatsby build",
    "develop": "SITE_COMMIT=\"${SITE_COMMIT:-$(git log -1 --oneline --decorate)}\" SITE_VERSION=\"${SITE_VERSION:-$(git rev-parse HEAD)}\" gatsby develop",
    "lint": "eslint '**/*.{js,jsx}' && prettier-eslint --list-different '**/*.{js,jsx}'",
    "lint:fix": "prettier-eslint --write '**/*.{js,jsx}'",
    "markdown:lint": "prettier-eslint --list-different 'content/**/*.md'",
    "markdown:lint:fix": "prettier-eslint --write 'content/**/*.md'",
    "new": "node src/utils/newpost.js",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "4.1.0",
    "bluebird": "3.5.3",
    "caniuse-db": "1.0.30000938",
    "chalk": "2.4.2",
    "color": "3.1.0",
    "compass-vertical-rhythm": "1.4.5",
    "feed": "2.0.2",
    "gatsby": "2.1.2",
    "gatsby-cli": "2.4.10",
    "gatsby-pagination": "1.2.0",
    "gatsby-plugin-catch-links": "2.0.11",
    "gatsby-plugin-flow": "1.0.2",
    "gatsby-plugin-glamor": "2.0.8",
    "gatsby-plugin-google-analytics": "2.0.13",
    "gatsby-plugin-react-helmet": "3.0.6",
    "gatsby-plugin-remove-trailing-slashes": "2.0.7",
    "gatsby-plugin-sharp": "2.0.20",
    "gatsby-plugin-sitemap": "2.0.5",
    "gatsby-plugin-svgr": "2.0.1",
    "gatsby-plugin-typography": "2.2.7",
    "gatsby-remark-abbr": "1.0.0",
    "gatsby-remark-autolink-headers": "2.0.14",
    "gatsby-remark-copy-linked-files": "2.0.9",
    "gatsby-remark-embed-snippet": "3.2.2",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-images": "3.0.3",
    "gatsby-remark-prismjs": "3.2.4",
    "gatsby-remark-responsive-iframe": "2.0.9",
    "gatsby-remark-smartypants": "2.0.8",
    "gatsby-source-filesystem": "2.0.20",
    "gatsby-transformer-remark": "2.2.5",
    "gatsby-transformer-sharp": "2.1.13",
    "gatsby-transformer-yaml": "2.1.8",
    "glamor": "2.20.40",
    "glamorous": "4.13.1",
    "gray-percentage": "2.0.0",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "pify": "4.0.1",
    "prismjs": "1.15.0",
    "prop-types": "15.7.2",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-fontawesome": "1.6.1",
    "react-gist": "1.2.1",
    "react-gravatar": "2.6.3",
    "react-helmet": "5.2.0",
    "react-typography": "0.16.18",
    "typography": "0.16.18",
    "typography-breakpoint-constants": "0.16.18",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "eslint": "5.13.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-simple-import-sort": "3.0.0",
    "eslint-plugin-standard": "4.0.0",
    "flow-bin": "0.92.1",
    "husky": "1.3.1",
    "inquirer": "6.2.2",
    "inquirer-datepicker-prompt": "0.4.2",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "prettier-eslint-cli": "4.7.1",
    "sort-package-json": "1.19.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  }
}
