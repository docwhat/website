{
  "name": "docwhat.org",
  "version": "1.0.0",
  "description": "Personal blog of Christian Höltje",
  "keywords": [
    "blog",
    "gatsby"
  ],
  "homepage": "https://github.com/docwhat/docwhat#readme",
  "bugs": {
    "url": "https://github.com/docwhat/docwhat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docwhat/docwhat.git"
  },
  "license": "MIT",
  "author": "Christian Höltje (https://docwhat.org/)",
  "main": "n/a",
  "scripts": {
    "build": "NODE_ENV=production script/env gatsby build",
    "clean": "gatsby clean",
    "cypress:open": "cypress open",
    "cypress:run": "wait-on http://localhost:8000 && cypress run --record=false",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "cypress:run:record": "cypress run --record",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "develop": "NODE_ENV=development script/env gatsby develop",
    "docker:build": "yarn build && script/compress && script/build",
    "docker:run": "docker container run --rm --detach --publish=8000:80 --name=docwhat.org docwhat:local",
    "docker:stop": "docker container rm -f docwhat.org",
    "lint": "yarn run lint:js && yarn run lint:css && yarn run lint:md && yarn run lint:text",
    "lint:css": "stylelint './src/**/*.{js,jsx,css}'",
    "lint:css:fix": "stylelint --fix './src/**/*.{js,jsx,css}'",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:css:fix && yarn run lint:md:fix && yarn run lint:text:fix",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint:js:fix": "prettier --write '**/*.{js,jsx}'",
    "lint:md": "remark --frail --quiet --no-stdout 'content/**/*.md'",
    "lint:md:fix": "prettier --write 'content/**/*.md'",
    "lint:md:watch": "remark 'content/**/*.md' --watch",
    "lint:text": "textlint 'content/**/*.md' '*.md'",
    "lint:text:fix": "textlint --fix 'content/**/*.md' '*.md'",
    "new": "node src/utils/newpost.js",
    "serve": "gatsby serve",
    "setup": "yarn install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "travis": "yarn setup && yarn clean && yarn lint && yarn docker:build && yarn docker:run && yarn cypress:run && yarn docker:stop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.md": [
        "prettier --write",
        "remark",
        "git add"
      ],
      "**/*.{js,jsx}": [
        "prettier --write",
        "eslint",
        "stylelint",
        "git add"
      ],
      "**/*.css": [
        "stylelint"
      ],
      "package.json": [
        "sort-package-json",
        "git add"
      ]
    },
    "relative": true
  },
  "dependencies": {
    "@babel/core": "7.4.4",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@svgr/webpack": "4.2.0",
    "bluebird": "3.5.4",
    "caniuse-db": "1.0.30000971",
    "chalk": "2.4.2",
    "color": "3.1.1",
    "eslint-plugin-emotion": "^10.0.7",
    "feed": "2.0.4",
    "gatsby": "2.7.0",
    "gatsby-cli": "2.6.2",
    "gatsby-image": "^2.0.41",
    "gatsby-pagination": "1.2.0",
    "gatsby-plugin-catch-links": "2.0.15",
    "gatsby-plugin-emotion": "^4.0.4",
    "gatsby-plugin-favicon": "^3.1.5",
    "gatsby-plugin-flow": "1.0.5",
    "gatsby-plugin-google-analytics": "2.0.20",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "2.0.37",
    "gatsby-plugin-sitemap": "2.1.0",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-remark-abbr": "1.0.0",
    "gatsby-remark-autolink-headers": "2.0.16",
    "gatsby-remark-copy-linked-files": "2.0.12",
    "gatsby-remark-embed-snippet": "3.2.4",
    "gatsby-remark-embed-youtube": "^0.0.7",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-images": "3.0.11",
    "gatsby-remark-prismjs": "3.2.9",
    "gatsby-remark-responsive-iframe": "2.1.1",
    "gatsby-remark-smartypants": "2.0.9",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "2.0.37",
    "gatsby-transformer-remark": "2.3.12",
    "gatsby-transformer-sharp": "2.1.19",
    "gatsby-transformer-yaml": "2.1.12",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "pify": "4.0.1",
    "prismjs": "1.16.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-fontawesome": "1.6.1",
    "react-gist": "1.2.1",
    "react-gravatar": "2.6.3",
    "react-helmet": "5.2.1",
    "shevyjs": "^1.1.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "cypress": "3.3.1",
    "cypress-testing-library": "3.0.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-flowtype": "3.9.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-simple-import-sort": "3.1.1",
    "eslint-plugin-standard": "4.0.0",
    "flow-bin": "0.98.1",
    "gatsby-remark-external-links": "0.0.4",
    "husky": "2.3.0",
    "inquirer": "6.3.1",
    "inquirer-datepicker-prompt": "0.4.2",
    "lint-staged": "8.1.7",
    "prettier": "1.17.1",
    "prettier-eslint-cli": "4.7.1",
    "prettier-stylelint": "0.4.2",
    "remark-abbr": "1.3.1",
    "remark-cli": "6.0.1",
    "remark-frontmatter": "1.3.1",
    "remark-preset-lint-recommended": "3.0.2",
    "sort-package-json": "1.22.1",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.7.0",
    "textlint": "11.2.5",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-date-weekday-mismatch": "1.0.5",
    "textlint-rule-diacritics": "0.0.2",
    "textlint-rule-editorconfig": "1.0.3",
    "textlint-rule-no-start-duplicated-conjunction": "2.0.2",
    "textlint-rule-no-todo": "2.0.1",
    "wait-on": "3.2.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  },
  "remarkConfig": {
    "settings": {
      "footnotes": true
    },
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-frontmatter",
      "remark-abbr"
    ]
  }
}
