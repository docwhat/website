{
  "name": "docwhat.org",
  "version": "1.0.0",
  "description": "Personal blog of Christian Höltje",
  "keywords": [
    "blog",
    "gatsby"
  ],
  "homepage": "https://github.com/docwhat/docwhat#readme",
  "bugs": {
    "url": "https://github.com/docwhat/docwhat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docwhat/docwhat.git"
  },
  "license": "MIT",
  "author": "Christian Höltje (https://docwhat.org/)",
  "main": "n/a",
  "scripts": {
    "build": "NODE_ENV=production script/env gatsby build",
    "clean": "gatsby clean",
    "compress": "script/compress",
    "cypress:open": "cypress open",
    "cypress:run": "wait-on http://localhost:8000 && cypress run",
    "cypress:run:chrome": "yarn run cypress:run --browser chrome",
    "cypress:run:headed": "yarn run cypress:run --headed",
    "cypress:run:record": "yarn run cypress:run --record",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "develop": "NODE_ENV=development script/env gatsby develop",
    "docker:build": "yarn build && yarn compress && script/build",
    "docker:run": "docker container run --rm --detach --publish=8000:80 --name=docwhat.org docwhat:local",
    "docker:stop": "docker container rm -f docwhat.org",
    "lint": "yarn run lint:js && yarn run lint:css && yarn run lint:md && yarn run lint:packagejson && yarn run lint:yarnlock",
    "lint:css": "stylelint './src/**/*.{js,jsx,css}'",
    "lint:css:fix": "stylelint --fix './src/**/*.{js,jsx,css}'",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:css:fix && yarn run lint:md:fix && yarn run lint:text:fix && yarn run lint:packagejson:fix && yarn run lint:yarnlock:fix",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint:js:fix": "prettier --write '**/*.{js,jsx}'",
    "lint:md": "remark --frail --quiet --no-stdout 'content/**/*.md'",
    "lint:md:fix": "prettier --write 'content/**/*.md'",
    "lint:md:watch": "remark 'content/**/*.md' --watch",
    "lint:packagejson": "yarn run sort-package-json --check",
    "lint:packagejson:fix": "yarn run sort-package-json",
    "lint:text": "textlint 'content/**/*.md' '*.md'",
    "lint:text:fix": "textlint --fix 'content/**/*.md' '*.md'",
    "lint:yarnlock": "yarn run yarn-deduplicate --list",
    "lint:yarnlock:fix": "yarn run yarn-deduplicate",
    "new": "node src/utils/newpost.js",
    "serve": "gatsby serve",
    "setup": "yarn install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "travis": "yarn setup && yarn clean && yarn lint && yarn docker:build && yarn docker:run && yarn cypress:run && yarn docker:stop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.css": [
      "stylelint"
    ],
    "**/*.md": [
      "prettier --write",
      "remark"
    ],
    "**/*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "stylelint"
    ],
    "package.json": [
      "yarn lint:packagejson:fix"
    ],
    "yarn.lock": [
      "yarn lint:yarnlock:fix"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-github",
      "remark-frontmatter",
      "remark-abbr"
    ],
    "settings": {
      "footnotes": true
    }
  },
  "dependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@svgr/webpack": "5.4.0",
    "bluebird": "3.7.2",
    "caniuse-db": "1.0.30001125",
    "chalk": "4.1.0",
    "color": "3.1.2",
    "fbjs": "2.0.0",
    "feed": "4.2.1",
    "gatsby": "2.24.58",
    "gatsby-cli": "2.12.95",
    "gatsby-image": "2.4.17",
    "gatsby-pagination": "1.2.0",
    "gatsby-plugin-catch-links": "2.3.11",
    "gatsby-plugin-emotion": "4.3.10",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-flow": "1.3.11",
    "gatsby-plugin-google-analytics": "2.3.13",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sharp": "2.6.35",
    "gatsby-plugin-sitemap": "2.4.12",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-remark-abbr": "2.0.0",
    "gatsby-remark-autolink-headers": "2.3.12",
    "gatsby-remark-copy-linked-files": "2.3.14",
    "gatsby-remark-embed-snippet": "4.3.14",
    "gatsby-remark-embed-youtube": "0.0.7",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "3.3.29",
    "gatsby-remark-prismjs": "3.5.11",
    "gatsby-remark-responsive-iframe": "2.4.13",
    "gatsby-remark-smartypants": "2.3.10",
    "gatsby-remark-source-name": "1.0.0",
    "gatsby-source-filesystem": "2.3.29",
    "gatsby-transformer-remark": "2.8.34",
    "gatsby-transformer-sharp": "2.5.14",
    "gatsby-transformer-yaml": "2.4.11",
    "lodash": "4.17.20",
    "moment": "2.27.0",
    "pify": "5.0.0",
    "prismjs": "1.21.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-fontawesome": "1.7.1",
    "react-gist": "1.2.2",
    "react-gravatar": "2.6.3",
    "react-helmet": "6.1.0",
    "react-refresh": "0.8.3",
    "shevyjs": "1.1.0",
    "strip-markdown": "3.1.2",
    "uuid": "8.3.0",
    "webpack": "4.44.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "6.0.0",
    "babel-eslint": "10.1.0",
    "cypress": "4.12.1",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-standard": "4.0.1",
    "flow-bin": "0.133.0",
    "gatsby-plugin-react-helmet-canonical-urls": "1.4.0",
    "gatsby-remark-external-links": "0.0.4",
    "husky": "4.3.0",
    "inquirer": "7.3.3",
    "inquirer-datepicker-prompt": "0.4.2",
    "lint-staged": "10.3.0",
    "object-assign": "4.1.1",
    "prettier": "2.1.1",
    "prettier-eslint-cli": "5.0.0",
    "prettier-stylelint": "0.4.2",
    "remark-abbr": "1.4.0",
    "remark-cli": "8.0.1",
    "remark-frontmatter": "2.0.0",
    "remark-github": "9.0.1",
    "remark-preset-github": "3.0.0",
    "sort-package-json": "1.44.0",
    "stylelint": "13.7.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "textlint": "11.7.6",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-date-weekday-mismatch": "1.0.5",
    "textlint-rule-diacritics": "1.0.0",
    "textlint-rule-editorconfig": "1.0.3",
    "textlint-rule-no-start-duplicated-conjunction": "2.0.2",
    "textlint-rule-no-todo": "2.0.1",
    "typescript": "4.0.2",
    "wait-on": "5.2.0",
    "yarn-deduplicate": "3.0.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  }
}
