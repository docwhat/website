{
  "name": "docwhat.org",
  "version": "1.0.0",
  "description": "Personal blog of Christian Höltje",
  "keywords": [
    "blog",
    "gatsby"
  ],
  "homepage": "https://github.com/docwhat/docwhat#readme",
  "bugs": {
    "url": "https://github.com/docwhat/docwhat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docwhat/docwhat.git"
  },
  "license": "MIT",
  "author": "Christian Höltje (https://docwhat.org/)",
  "main": "n/a",
  "scripts": {
    "build": "NODE_ENV=production script/env gatsby build",
    "clean": "gatsby clean",
    "compress": "script/compress",
    "cypress:open": "cypress open",
    "cypress:run": "wait-on http://localhost:8000 && cypress run",
    "cypress:run:chrome": "yarn run cypress:run --browser=chrome",
    "cypress:run:firefox": "yarn run cypress:run --browser=firefox",
    "cypress:run:headed": "yarn run cypress:run --headed",
    "cypress:run:record": "yarn run cypress:run --record",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "develop": "NODE_ENV=development script/env gatsby develop",
    "docker:build": "yarn build && yarn compress && script/build",
    "docker:run": "docker container run --rm --detach --publish=8000:80 --name=docwhat.org docwhat:local",
    "docker:stop": "docker container rm -f docwhat.org",
    "format": "yarn format:prettier && yarn format:packagejson && yarn format:yarnlock",
    "format:fix": "yarn format:prettier:fix && yarn format:packagejson:fix && yarn format:yarnlock:fix",
    "format:prettier": "prettier --check '**/*.{js,jsx,md,yml,yaml,css,graphql,json,json5,html}'",
    "format:prettier:fix": "prettier --write '**/*.{js,jsx,md,yml,yaml,css,graphql,json,json5,html}'",
    "format:packagejson": "yarn run sort-package-json --check",
    "format:packagejson:fix": "yarn run sort-package-json",
    "format:yarnlock": "yarn run yarn-deduplicate --list",
    "format:yarnlock:fix": "yarn run yarn-deduplicate",
    "lint": "node --version && yarn lint:eslint && yarn lint:stylelint && yarn lint:textlint",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:stylelint:fix && yarn lint:textlint:fix",
    "lint:stylelint": "stylelint './**/*.{js,jsx,html,css}'",
    "lint:stylelint:fix": "stylelint --fix './**/*.{js,jsx,html,css}'",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:textlint": "textlint .",
    "lint:textlint:fix": "textlint --fix .",
    "new": "node src/utils/newpost.js",
    "serve": "gatsby serve",
    "setup": "yarn install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "travis": "yarn setup && yarn clean && yarn lint && yarn docker:build && yarn docker:run && yarn cypress:run && yarn docker:stop",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.css": [
      "stylelint --fix"
    ],
    "**/*.md": [
      "textlint --fix",
      "prettier --write",
      "remark"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "package.json": [
      "yarn format:packagejson:fix"
    ],
    "yarn.lock": [
      "yarn format:yarnlock:fix"
    ]
  },
  "resolutions": {
    "**/graphql-config/**/ws": "^7.4.6",
    "**/remark-parse/trim": "^1.0.0",
    "gatsby/react-dev-utils/browserslist": "^4.16.5"
  },
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.16.7",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@svgr/webpack": "^5.5.0",
    "bluebird": "~3.7.2",
    "caniuse-db": "~1.0.30001270",
    "chalk": "^4.1.2",
    "color": "^4.0.1",
    "fbjs": "~3.0.1",
    "feed": "~4.2.2",
    "gatsby": "^3.14.4",
    "gatsby-cli": "^3.14.2",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-emotion": "^6.14.0",
    "gatsby-plugin-google-analytics": "^3.14.0",
    "gatsby-plugin-image": "^1.14.1",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^3.14.1",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-svgr": "~3.0.0-beta.0",
    "gatsby-remark-autolink-headers": "^4.11.0",
    "gatsby-remark-copy-linked-files": "^4.11.0",
    "gatsby-remark-embed-snippet": "^6.11.0",
    "gatsby-remark-embed-youtube": "0.0.7",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-remark-responsive-iframe": "^4.11.0",
    "gatsby-remark-smartypants": "^4.11.0",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "gatsby-transformer-yaml": "^3.14.0",
    "lodash": "~4.17.21",
    "moment": "~2.29.1",
    "pify": "~5.0.0",
    "prismjs": "~1.25.0",
    "prop-types": "~15.7.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-fontawesome": "~1.7.1",
    "react-gravatar": "~2.6.3",
    "react-helmet": "~6.1.0",
    "react-hot-loader": "~4.13.0",
    "react-refresh": "0.11.0",
    "replace-ext": "~2.0.0",
    "shevyjs": "^2.1.0",
    "strip-markdown": "~5.0.0",
    "uuid": "~8.3.2",
    "vfile": "~5.2.1",
    "webpack": "~5.52.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "~11.5.0",
    "@testing-library/cypress": "~8.0.2",
    "babel-eslint": "^10.1.0",
    "cypress": "~8.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gatsby-plugin-react-helmet-canonical-urls": "~1.4.0",
    "gatsby-remark-external-links": "0.0.4",
    "husky": "^7.0.4",
    "inquirer": "~8.2.0",
    "inquirer-datepicker-prompt": "0.4.2",
    "lint-staged": "~11.2.3",
    "object-assign": "~4.1.1",
    "prettier": "~2.4.1",
    "remark-cli": "~10.0.0",
    "remark-frontmatter": "~4.0.0",
    "remark-github": "~11.1.1",
    "remark-preset-github": "~4.0.1",
    "sort-package-json": "~1.52.0",
    "stylelint": "~13.13.1",
    "stylelint-config-prettier": "~9.0.3",
    "stylelint-config-recommended": "~5.0.0",
    "stylelint-prettier": "~1.2.0",
    "textlint": "~12.0.2",
    "textlint-filter-rule-comments": "~1.2.2",
    "textlint-rule-date-weekday-mismatch": "~1.0.6",
    "textlint-rule-diacritics": "~1.0.0",
    "textlint-rule-editorconfig": "~1.0.3",
    "textlint-rule-no-start-duplicated-conjunction": "~2.0.2",
    "textlint-rule-no-todo": "~2.0.1",
    "typescript": "~4.5.3",
    "wait-on": "~6.0.0",
    "yarn-deduplicate": "~3.1.0"
  }
}
