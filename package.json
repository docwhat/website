{
  "name": "docwhat.org",
  "version": "1.0.0",
  "description": "Personal blog of Christian Höltje",
  "keywords": [
    "blog",
    "gatsby"
  ],
  "homepage": "https://github.com/docwhat/docwhat#readme",
  "bugs": {
    "url": "https://github.com/docwhat/docwhat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docwhat/docwhat.git"
  },
  "license": "MIT",
  "author": "Christian Höltje (https://docwhat.org/)",
  "main": "n/a",
  "scripts": {
    "build": "NODE_ENV=production script/env gatsby build",
    "clean": "gatsby clean",
    "compress": "script/compress",
    "cypress:open": "cypress open",
    "cypress:run": "wait-on http://localhost:8000 && cypress run",
    "cypress:run:chrome": "yarn run cypress:run --browser chrome",
    "cypress:run:headed": "yarn run cypress:run --headed",
    "cypress:run:record": "yarn run cypress:run --record",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "develop": "NODE_ENV=development script/env gatsby develop",
    "docker:build": "yarn build && yarn compress && script/build",
    "docker:run": "docker container run --rm --detach --publish=8000:80 --name=docwhat.org docwhat:local",
    "docker:stop": "docker container rm -f docwhat.org",
    "lint": "yarn run lint:js && yarn run lint:css && yarn run lint:md && yarn run lint:packagejson && yarn run lint:yarnlock",
    "lint:css": "stylelint './src/**/*.{js,jsx,css}'",
    "lint:css:fix": "stylelint --fix './src/**/*.{js,jsx,css}'",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:css:fix && yarn run lint:md:fix && yarn run lint:text:fix && yarn run lint:packagejson:fix && yarn run lint:yarnlock:fix",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint:js:fix": "prettier --write '**/*.{js,jsx}'",
    "lint:md": "remark --frail --quiet --no-stdout 'content/**/*.md'",
    "lint:md:fix": "prettier --write 'content/**/*.md'",
    "lint:md:watch": "remark 'content/**/*.md' --watch",
    "lint:packagejson": "yarn run sort-package-json --check",
    "lint:packagejson:fix": "yarn run sort-package-json",
    "lint:text": "textlint 'content/**/*.md' '*.md'",
    "lint:text:fix": "textlint --fix 'content/**/*.md' '*.md'",
    "lint:yarnlock": "yarn run yarn-deduplicate --list",
    "lint:yarnlock:fix": "yarn run yarn-deduplicate",
    "new": "node src/utils/newpost.js",
    "serve": "gatsby serve",
    "setup": "yarn install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "travis": "yarn setup && yarn clean && yarn lint && yarn docker:build && yarn docker:run && yarn cypress:run && yarn docker:stop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.css": [
      "stylelint"
    ],
    "**/*.md": [
      "prettier --write",
      "remark"
    ],
    "**/*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "stylelint"
    ],
    "package.json": [
      "yarn lint:packagejson:fix"
    ],
    "yarn.lock": [
      "yarn lint:yarnlock:fix"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-github",
      "remark-frontmatter"
    ],
    "settings": {
      "footnotes": true
    }
  },
  "dependencies": {
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@svgr/webpack": "5.5.0",
    "bluebird": "3.7.2",
    "caniuse-db": "1.0.30001230",
    "chalk": "4.1.1",
    "color": "3.1.3",
    "fbjs": "3.0.0",
    "feed": "4.2.2",
    "gatsby": "2.32.13",
    "gatsby-cli": "3.5.0",
    "gatsby-image": "3.5.0",
    "gatsby-pagination": "1.2.0",
    "gatsby-plugin-catch-links": "2.10.0",
    "gatsby-plugin-emotion": "5.4.0",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-flow": "1.10.0",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sharp": "2.14.4",
    "gatsby-plugin-sitemap": "2.12.0",
    "gatsby-plugin-svgr": "2.1.0",
    "gatsby-remark-autolink-headers": "2.11.0",
    "gatsby-remark-copy-linked-files": "2.10.0",
    "gatsby-remark-embed-snippet": "4.10.0",
    "gatsby-remark-embed-youtube": "0.0.7",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "3.11.1",
    "gatsby-remark-prismjs": "3.13.0",
    "gatsby-remark-responsive-iframe": "2.11.0",
    "gatsby-remark-smartypants": "2.10.0",
    "gatsby-remark-source-name": "1.0.0",
    "gatsby-source-filesystem": "2.11.1",
    "gatsby-transformer-remark": "2.16.1",
    "gatsby-transformer-sharp": "2.12.1",
    "gatsby-transformer-yaml": "2.11.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "pify": "5.0.0",
    "prismjs": "1.23.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-fontawesome": "1.7.1",
    "react-gist": "1.2.2",
    "react-gravatar": "2.6.3",
    "react-helmet": "6.1.0",
    "react-hot-loader": "4.13.0",
    "react-refresh": "0.10.0",
    "shevyjs": "1.2.0",
    "strip-markdown": "4.0.0",
    "uuid": "8.3.2",
    "webpack": "4.46.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.2.0",
    "@testing-library/cypress": "7.0.6",
    "babel-eslint": "10.1.0",
    "cypress": "6.9.1",
    "eslint": "7.22.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-flowtype": "5.2.2",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-standard": "5.0.0",
    "flow-bin": "0.151.0",
    "gatsby-plugin-react-helmet-canonical-urls": "1.4.0",
    "gatsby-remark-external-links": "0.0.4",
    "husky": "6.0.0",
    "inquirer": "8.0.0",
    "inquirer-datepicker-prompt": "0.4.2",
    "lint-staged": "11.0.0",
    "object-assign": "4.1.1",
    "prettier": "2.2.1",
    "prettier-eslint-cli": "5.0.1",
    "prettier-stylelint": "0.4.2",
    "remark-cli": "9.0.0",
    "remark-frontmatter": "3.0.0",
    "remark-github": "10.1.0",
    "remark-preset-github": "4.0.1",
    "sort-package-json": "1.49.0",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "textlint": "11.9.1",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-date-weekday-mismatch": "1.0.5",
    "textlint-rule-diacritics": "1.0.0",
    "textlint-rule-editorconfig": "1.0.3",
    "textlint-rule-no-start-duplicated-conjunction": "2.0.2",
    "textlint-rule-no-todo": "2.0.1",
    "typescript": "4.2.4",
    "wait-on": "5.2.1",
    "yarn-deduplicate": "3.1.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  }
}
